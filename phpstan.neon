includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    checkMissingIterableValueType: true

    level: 3

    paths:
        - app
        - config
        - bootstrap
        - database/factories
        - routes

    excludePaths:
        - app/Http/ViewModels/CheckinViewModel.php

    ignoreErrors:
        - '#Access to an undefined property App\\Models\\User::\$profile_photo_url#'
